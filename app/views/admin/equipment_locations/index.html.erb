<h1>Listing equipment locations</h1>

<table class="normal_table">
  <tr>
    <th>name</th>
  </tr>

<% @equipment_locations.each do |equipment_location| %>
  <tr>
    <td><%=h equipment_location.name %></td>
    <td><%= link_to 'Show', [:admin, equipment_location] %></td>
    <td><%= link_to 'Edit', edit_admin_equipment_location_path(equipment_location) %></td>
    <td><%= link_to 'Destroy', [:admin, equipment_location], :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New equipment location', new_admin_equipment_location_path %>