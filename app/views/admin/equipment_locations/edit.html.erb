<h1>Editing equipment location</h1>

<% form_for([:admin, @equipment_location]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', [:admin, @equipment_location] %> |
<%= link_to 'Back', admin_equipment_locations_path %>