<h1>Listing equipment miscellaneous</h1>

<table class="normal_table">
  <tr>
    <th>Type</th>
    <th>Brand</th>
    <th>Model</th>
    <th># Serial</th>
    <th># Inventory</th>
    <th>Location</th>
    <th>Department</th>
  </tr>

<% @equipment_miscellaneous.each do |equipment_miscellaneou| %>
  <tr>
    <td><%=h equipment_miscellaneou.equipment_type.name %></td>
    <td><%=h equipment_miscellaneou.brand %></td>
    <td><%=h equipment_miscellaneou.model %></td>
    <td><%=h equipment_miscellaneou.serial_number %></td>
    <td><%=h equipment_miscellaneou.inventory_number %></td>
    <td><%=h equipment_miscellaneou.equipment_location.name %></td>
    <td><%=h equipment_miscellaneou.department.name %></td>
    <td><%= link_to 'Show', [:admin, equipment_miscellaneou] %></td>
    <td><%= link_to 'Edit', edit_admin_equipment_miscellaneou_path(equipment_miscellaneou) %></td>
    <td><%= link_to 'Take out', "/admin/equipment_miscellaneous/take_out/#{equipment_miscellaneou.id}"  %></td>
    <!--
    <td><%#= link_to 'Destroy', [:admin, equipment_miscellaneou], :confirm => 'Are you sure?', :method => :delete %></td>
    -->
  </tr>
<% end %>
</table>
<%= will_paginate @equipment_miscellaneous %>
<br />

<%= link_to 'New equipment miscellaneou', new_admin_equipment_miscellaneou_path %>