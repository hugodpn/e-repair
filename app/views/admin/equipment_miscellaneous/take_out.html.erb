<h1>Take out</h1>

<% form_for  :person, :url => { :action => "take_out_up", :id => @equipment_miscellaneou.id } do |f| %>
  <%= hidden_field_tag :take_out_user_id, current_user.id %><br />
  <b>Reason:</b><br />
  <%= text_area_tag :take_out_reason, "", :size => "40x4" %><br />
  <%= f.submit "Take out" %>
<% end %> 

